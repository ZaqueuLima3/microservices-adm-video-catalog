{
  "compilerOptions": {
    "incremental": true,
    "target": "es2017",
    "experimentalDecorators": true,
    "module": "commonjs",
    "baseUrl": "./src",
    "paths": {
      "#seedwork/*": [
        "@seedwork/*"
      ],
      "#category/*": [
        "category/*"
      ]
    },
    "outDir": "dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "strictNullChecks": false,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "declaration": true
  },
  "include": ["src/"]
}
